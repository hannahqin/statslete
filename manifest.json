{
  "manifest_version": 2,

  "name": "STATSLETE",
  "description": "woooooo go statslete",
  "version": "1.0.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "statslete.html"
  },

  "icons": {
    "16": "16icon.png"
  },

  "content_security_policy": "script-src 'self' https://stats.nba.com; object-src 'self'",

  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "notifications",
    "http://*/*",
    "https://*/*",
    "https://stats.nba.com/*"
  ],

  "background": {
    "scripts": ["scripts/statslete.js"]
  },

  "content_scripts": [
    {
      "hmmm"
    }

  ]

  }

}
